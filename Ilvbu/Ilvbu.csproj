    <Product>Ilvbu</Product>
    <Description>ilvbu</Description>
    <PackageId>Ilvbu</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>ilvbu</PackageReleaseNotes>
